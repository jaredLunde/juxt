{
  "env": {
    "commonjs": {
      "presets": [
        ["@babel/env", {"loose": true}]
      ],
      "plugins": [
        "closure-elimination",
        "dev-expression"
      ]
    },
    "es": {
      "presets": [
        ["@babel/env", {"loose": true, "modules": false}]
      ],
      "plugins": [
        "closure-elimination",
        "dev-expression"
      ]
    }
  }
}
